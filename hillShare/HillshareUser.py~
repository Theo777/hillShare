class User:
	def __init__(self, userid, password, studentEmail, profPic, groups):
		self.userid=userid
		self.password=password
		self.studentEmail=studentEmail
		self.profPic=profPic
		self.groups=groups
		
	def toString(self):
		return self.userid+" "+self.password+" "+self.studentEmail+" " +self.profPic
		
